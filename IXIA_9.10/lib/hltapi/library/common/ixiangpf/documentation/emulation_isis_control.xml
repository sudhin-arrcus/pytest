    <example lang="robot" supportType="ixNetworkCpfSupport"><![CDATA[
# Command :- Emulation Isis Control
# This example will start the ISIS 

   ${result} =  Emulation Isis Control  mode=start   handle=<isis_handle_1>  
   ${status} =  Get From Dictionary  ${result}  status
   Run Keyword If  '${status}' != '1'  FAIL  "Error: Status is not SUCCESS"  ELSE  Log  "Status is SUCCESS"
   ${isis_started} =  Get From Dictionary  ${result}  isis_started


   ${result} =  Emulation Isis Control  mode=start   handle=<isis_handle_2>  
   ${status} =  Get From Dictionary  ${result}  status
   Run Keyword If  '${status}' != '1'  FAIL  "Error: Status is not SUCCESS"  ELSE  Log  "Status is SUCCESS"
   ${isis_started} =  Get From Dictionary  ${result}  isis_started

# This example will stop hello packets transmission

   ${result} =  Emulation Isis Control  mode=stop_hello   handle=<isis_handle_1>  
   ${status} =  Get From Dictionary  ${result}  status
   Run Keyword If  '${status}' != '1'  FAIL  "Error: Status is not SUCCESS"  ELSE  Log  "Status is SUCCESS"
   ${hello_stopped} =  Get From Dictionary  ${result}  hello_stopped

# All the commands will have the output as shown below:
# {'status' : '1'  
	]]></example>
