    <example lang="tcl" supportType="ixNetworkCpfSupport"><![CDATA[
# Command :- ::ixiangpf::emulation_isis_control

# This example will start the ISIS 

set isis_started [::ixiangpf::emulation_isis_control 	    \
	-mode   		  start 		    \
	-handle			  <isis_handle_1> 	    \
]

set isis_started [::ixiangpf::emulation_isis_control 	    \
	-mode   		  start 		    \
	-handle 		  <isis_handle_2>	    \
]

# This example will stop hello packets transmission

set hello_stopped [::ixiangpf::emulation_isis_control 	    \
	-mode 			  stop_hello 		    \
	-handle 		  <isis_handle_1> 	    \
]

# All the commands will have the output as shown below:
# {status 1}
	]]></example>
