  <code_examples>
    <example lang="tcl" supportType="ixNetworkCpfSupport"><![CDATA[
# Command :- ::ixiangpf::traffic_tag_config

# This command will a traffic tag for any valid endpoint

set tag_1_status [::ixiangpf::traffic_tag_config 	\
        -handle       <endpoint_handle>      		\
        -enabled      1                   		\
        -name         TAG                 		\
        -id           1                   		\
]

# This command will generate output as
# {status 1} {traffic_tag_handle /topology:1/deviceGroup:1/ethernet:1/ipv4:1/tag:1}
    ]]></example>
    <example lang="python" supportType="ixNetworkCpfSupport"><![CDATA[
# Command :- ixiangpf.traffic_tag_config
# This command will a traffic tag for any valid endpoint

status = ixiangpf.traffic_tag_config(
    handle                                                      = "<endpoint_handle>",
    enabled                                                     = "1",
    name                                                        = "TAG",
    id                                                          = "1",
)
# This command will generate output as
# {'status' : '1', 'traffic_tag_handle' : 'trafficTaghandle'}
    ]]></example>
    <example lang="perl" supportType="ixNetworkCpfSupport"><![CDATA[
# Command :- ixiangpf::traffic_tag_config
# This command will a traffic tag for any valid endpoint
my $tag_1_status = ixiangpf::traffic_tag_config ({
    handle                                                      => "<endpoint_handle>",
    enabled                                                     => "1",
    name                                                        => "TAG",
    id                                                          => "1",
});
# This command will generate output as
# {status 1} {traffic_tag_handle /topology:1/deviceGroup:1/ethernet:1/ipv4:1/tag:1}
	    ]]></example>
    <example lang="robot" supportType="ixNetworkCpfSupport"><![CDATA[
# Command :- Traffic Tag Config 
# This command will a traffic tag for any valid endpoint

   ${result} =  Traffic Tag Config  handle=<endpoint_handle>   enabled=1   name=TAG   id=1  
   ${status} =  Get From Dictionary  ${result}  status
   Run Keyword If  '${status}' != '1'  FAIL  "Error: Status is not SUCCESS"  ELSE  Log  "Status is SUCCESS"
   ${tag_1_status} =  Get From Dictionary  ${result}  tag_1_status

# This command will generate output as
# {'status' : '1', 'traffic_tag_handle' : 'trafficTaghandle'}
    ]]></example>
