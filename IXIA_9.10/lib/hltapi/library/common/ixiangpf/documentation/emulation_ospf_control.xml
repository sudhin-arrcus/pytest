  <code_examples>
    <example lang="tcl" supportType="ixNetworkCpfSupport"><![CDATA[
# Command :- ::ixiangpf::emulation_ospf_control

# This example will start an OSPF Router

set ospf_started [::ixiangpf::emulation_ospf_control 	    \
	-mode   		  start 		    \
	-handle 		  <ospf_handle_1> 	    \
]

# This command will withdraw/age out routes

set route_range_withdrawn [::ixiangpf::emulation_ospf_control 	\
	-mode                     age_out_routes 		\
	-handle           	  <ospfRouteProperty> 		\
	-age_out_percent  	  10 				\
]

# Both command above will generate output as shown below:
# {status 1}
	]]></example>
    <example lang="python" supportType="ixNetworkCpfSupport"><![CDATA[
# Command :- ::ixiangpf.emulation_ospf_control
# This example will start an OSPF Router

status = ixiangpf.emulation_ospf_control(
    mode                                                        = "start",
    handle                                                      = "<ospf_handle_1>",
)
# This command will withdraw/age out routes

status = ixiangpf.emulation_ospf_control(
    mode                                                        = "age_out_routes",
    handle                                                      = "<ospfRouteProperty>",
    age_out_percent                                             = "10",
)
# Both command above will generate output as shown below:
# {'status' : '1'}

    ]]></example>
    <example lang="perl" supportType="ixNetworkCpfSupport"><![CDATA[
# Command :- ixiangpf::emulation_ospf_control
# This example will start an OSPF Router
my $ospf_started = ixiangpf::emulation_ospf_control ({
    mode                                                        => "start",
    handle                                                      => "<ospf_handle_1>",
});
# This command will withdraw/age out routes
my $route_range_withdrawn = ixiangpf::emulation_ospf_control ({
    mode                                                        => "age_out_routes",
    handle                                                      => "<ospfRouteProperty>",
    age_out_percent                                             => "10",
});
# Both command above will generate output as shown below:
# {status 1}
	]]></example>
    <example lang="robot" supportType="ixNetworkCpfSupport"><![CDATA[
# Command :-  Emulation Ospf Control
# This example will start an OSPF Router

   ${result} =  Emulation Ospf Control  mode=start   handle=<ospf_handle_1>  
   ${status} =  Get From Dictionary  ${result}  status
   Run Keyword If  '${status}' != '1'  FAIL  "Error: Status is not SUCCESS"  ELSE  Log  "Status is SUCCESS"
   ${ospf_started} =  Get From Dictionary  ${result}  ospf_started

# This command will withdraw/age out routes

   ${result} =  Emulation Ospf Control  mode=age_out_routes   handle=<ospfRouteProperty>   age_out_percent=10  
   ${status} =  Get From Dictionary  ${result}  status
   Run Keyword If  '${status}' != '1'  FAIL  "Error: Status is not SUCCESS"  ELSE  Log  "Status is SUCCESS"
   ${route_range_withdrawn} =  Get From Dictionary  ${result}  route_range_withdrawn

# Both command above will generate output as shown below:
# {'status' : '1'} 
	]]></example>
