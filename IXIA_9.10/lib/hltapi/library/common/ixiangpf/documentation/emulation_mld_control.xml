          <choices>
            <choice>
              <choiceName>start</choiceName>
              <choiceText>Starts the MLD host,  requires the use of -handle.</choiceText>
            </choice>
            <choice>
              <choiceName>stop</choiceName>
              <choiceText>Stops the MLD host,  requires the use of -handle.</choiceText>
            </choice>
            <choice>
