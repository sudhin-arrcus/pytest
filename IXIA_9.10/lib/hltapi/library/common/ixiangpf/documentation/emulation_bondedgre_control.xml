            </choice>
            <choice>
              <choiceName>dslLinkFailure</choiceName>
              <choiceText>Sends a Message for Emulating DSL Link Failure.</choiceText>
            </choice>
            <choice>
              <choiceName>lteLinkFailure</choiceName>
              <choiceText>Sends a Message for Emulating LTE Link Failure.</choiceText>
            </choice>
            <choice>
              <choiceName>tearDown</choiceName>
              <choiceText>Sends a Message for Emulating Tunnel Tear down.</choiceText>
            </choice>
            <choice>
              <choiceName>resumehello</choiceName>
              <choiceText>Sends a Message for Emulating Resume hello.</choiceText>
            </choice>
            <choice>
              <choiceName>stophello</choiceName>
              <choiceText>Sends a Message for Emulating Stop hello.</choiceText>
            </choice>
          </choices>
