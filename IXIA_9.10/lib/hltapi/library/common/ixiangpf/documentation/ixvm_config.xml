          </choices>
        </description>
        <restriction type="VALUETYPE" valueType="CHOICES" value="create modify delete delete_all" />
      </ixNetworkCpfSupport>
      <ixLoadSupport contentType="noContent" target="noTarget" mandatory="false" />
    </argument>
